package mini;

//카드 저장 클래스
public class Card{
	String color;
	String number;
	
	
	public Card(String color, String number) {
		this.color = color;
		this.number = number;
	}
	
	//Card객체에 저장된 데이터를 표현하기 위해 toString 메소드를 활용
	@Override
	public String toString() {
		return "(" + color + " " + number + ")";
	}
		
}